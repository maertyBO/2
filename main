#include <iostream>
#include <algorithm>

int main() {
    char alphabet[] = {'c', 'a', 'b', 'e', 'd', 'g', 'f'};
    int size = sizeof(alphabet) / sizeof(alphabet[0]);

    std::sort(alphabet, alphabet + size);
    
    for (int i = 0; i < size; i++) {
        std::cout << alphabet[i] << " ";
    }
    // Output: a b c d e f g

    return 0;
}
